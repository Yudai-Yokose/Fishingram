<section class="bg-gray-50 dark:bg-gray-900">
  <div class="flex flex-col items-center justify-center px-6 mx-auto">
    <div class="w-full bg-white rounded-lg shadow dark:border md:mt-0 sm:max-w-md xl:p-0 dark:bg-gray-900 dark:border-gray-700">
      <div class="p-6 space-y-3 md:space-y-6 sm:p-8">
        <!-- title -->
        <span class="text-2xl font-extrabold tracking-tight">All</span>
        <span class="ms-1 text-base font-normal text-gray-500 dark:text-gray-400">catches</span>
        <!-- list of catches -->
        <div class="flow-root">
          <ul role="list" class="divide-y divide-gray-200 dark:divide-gray-700" id="catches">
            <% @catches.each do |catch| %>
              <div id="destroy_catch_<%= catch.id %>">
                <%= render partial: 'catch', locals: { catch: catch } %>
              </div>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
</section>

